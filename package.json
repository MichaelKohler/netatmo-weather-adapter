{
  "name": "netatmo-weather-adapter",
  "display_name": "Netatmo Weather Station",
  "version": "0.3.6",
  "description": "WoT adapter for netatmo weather stations",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "licenses": "./gather-licenses.js",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-hellhake/netatmo-weather-adapter.git"
  },
  "keywords": [
    "adapter",
    "webofthings",
    "thing",
    "wot",
    "iot",
    "netatmo",
    "weather"
  ],
  "author": "Tim Hellhake",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tim-hellhake/netatmo-weather-adapter/issues"
  },
  "homepage": "https://github.com/tim-hellhake/netatmo-weather-adapter#readme",
  "files": [
    "manifest.json",
    "lib/index.js",
    "lib/adapter.js",
    "LICENSE",
    "SHA256SUMS"
  ],
  "moziot": {
    "api": {
      "min": 2,
      "max": 2
    },
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "schema": {
      "type": "object",
      "title": "Login to your Netatmo Smart Home",
      "properties": {
        "client_id": {
          "type": "string",
          "title": "API Client ID"
        },
        "client_secret": {
          "type": "string",
          "title": "API Client Secret"
        },
        "username": {
          "type": "string",
          "title": "Username"
        },
        "password": {
          "type": "string",
          "title": "Password"
        }
      }
    }
  },
  "dependencies": {
    "netatmo": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.8",
    "nlf": "^2.1.1",
    "typescript": "^3.7.5"
  }
}
